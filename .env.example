# OPENAI_API_KEY=sk-...

# Google (Gemini)
# GEMINI_API_KEY=...

# Groq (Ultra-fast inference)
# GROQ_API_KEY=gsk_...

# xAI (Grok)
# XAI_API_KEY=...

# Fireworks AI
# FIREWORKS_API_KEY=...

# Moonshot AI (Kimi)
